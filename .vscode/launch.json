    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Node:Nodemon",
            "processId": "${command:PickProcess}",
            "restart": true,
            "protocol": "inspector",
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
